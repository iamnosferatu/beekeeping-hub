{
  "name": "beekeeper-backend",
  "version": "1.0.0",
  "description": "Backend for BeeKeeping Hub",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "seed": "node src/seeds/seed.js",
    "migrate": "sequelize-cli db:migrate",
